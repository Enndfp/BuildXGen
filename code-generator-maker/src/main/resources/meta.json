{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.enndfp",
  "version": "1.0",
  "author": "enndfp",
  "createTime": "2024-02-05",
  "forcedInteractive": false,
  "fileConfig": {
    "inputRootPath": ".source/acm-template-pro",
    "outputRootPath": "generated",
    "sourceRootPath": "E:/planet/code-generator/code-generator-demo-projects/acm-template-pro",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/enndfp/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/enndfp/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": true,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者名称",
        "defaultValue": "Enndfp",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}